{
  "version": "1.0.13",
  "keywords": [],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "admin/src",
    "server/src",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "jodit": "^4.6.2",
    "jodit-react": "^5.2.19"
  },
  "devDependencies": {
    "@strapi/design-system": "^2.0.0-rc.29",
    "@strapi/icons": "^2.0.0-rc.29",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.22.0",
    "@strapi/typescript-utils": "^5.22.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^7.1.11",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.20.2",
    "@esbuild/darwin-x64": "^0.20.2",
    "@esbuild/linux-arm64": "^0.20.2",
    "@esbuild/linux-x64": "^0.20.2",
    "@esbuild/win32-x64": "^0.20.2"
  },
  "peerDependencies": {
    "@strapi/design-system": "^2.0.0-rc.29",
    "@strapi/icons": "^2.0.0-rc.29",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^7.1.11",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "jodit-editor",
    "displayName": "Jodit Editor",
    "description": "A powerful rich text editor plugin for Strapi v5 using the Jodit Editor with advanced formatting capabilities and seamless integration with Strapi's media library."
  },
  "name": "strapi-plugin-jodit-editor",
  "description": "A powerful rich text editor plugin for Strapi v5 using the Jodit Editor with advanced formatting capabilities and seamless integration with Strapi's media library.",
